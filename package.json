{
  "name": "mava",
  "version": "1.0.0",
  "description": "Mava backend",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:integration": "jest tests/integration",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server/index.ts",
    "build": "rm -rf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "tsc-check": "tsc --noEmit -p tsconfig.json && tsc --noEmit",
    "eslint-check": "eslint \"src/**/*.ts\"",
    "eslint-fix": "eslint \"{src}/**/*.ts\" --fix",
    "fix-yaml": "prettier --write '**/*.(yaml|yml)'",
    "check-yaml": "prettier --check '**/*.(yaml|yml)'",
    "prettier-check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "prettier-fix": "prettier --write \"**/*.{js,ts,json,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Extheoisah/Mava.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
