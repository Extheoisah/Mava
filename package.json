  },
  "homepage": "https://github.com/Extheoisah/Mava#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.8.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@types/validator": "^13.11.1",
